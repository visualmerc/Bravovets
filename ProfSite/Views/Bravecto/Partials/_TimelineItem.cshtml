@model FacebookTimelinePost

@if (Model.Type == "status")
{
    @Html.Partial("Partials/StatusUpdate", Model)
}
else if (Model.Type == "link")
{
    @Html.Partial("Partials/LinkUpdate", Model)
}
else if (Model.Type == "photo")
{
    @Html.Partial("Partials/PhotoUpdate", Model)
}
else if (Model.Type == "swf" || Model.Type == "video")
{
    @Html.Partial("Partials/VideoUpdate", Model)
}

