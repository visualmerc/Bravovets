@model SyndicatedShareLinkModel

@if (Model.ShowShare)
{
    <a href="#" data-event-id="@Model.SyndicatedContentId" class="share-this share @(!Model.EnableShare?"disabled":string.Empty)">
        @Resource.BravoVets_TrendingTopicItem_Share this
        <i class="icon icon-share"></i>
    </a>
}
